    "tape": "^4.2.0",
    "zuul": "^3.6.0",
    "@nathanfaucett/values": "^0.0.1"
  },
  "dependencies": {
    "@nathanfaucett/array-for_each": "^0.0.1",
    "@nathanfaucett/event_emitter": "^0.0.1",
    "@nathanfaucett/has": "^0.0.1",
    "@nathanfaucett/inherits": "^0.0.1",
    "@nathanfaucett/is_array": "^0.0.1",
