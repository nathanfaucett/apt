{
  "name": "apt",
  "version": "0.0.1",
  "description": "apt is a framework for building applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/apt.git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 9999 --ui tape -- ./test"
  },
  "keywords": [
    "apt",
    "apt.js"
  ],
  "author": "Nathan Faucett",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-jsbeautifier": "^0.2.7",
    "tap-spec": "^4.0.0",
    "tape": "^4.0.0",
    "values": "git://github.com/nathanfaucett/values.git",
    "zuul": "^3.0.0"
  },
  "dependencies": {
    "array-for_each": "git://github.com/nathanfaucett/array-for_each.git",
    "event_emitter": "git://github.com/nathanfaucett/event_emitter.git",
    "has": "git://github.com/nathanfaucett/has.git",
    "inherits": "git://github.com/nathanfaucett/inherits.git",
    "is_array": "git://github.com/nathanfaucett/is_array.git",
    "is_function": "git://github.com/nathanfaucett/is_function.git",
    "is_string": "git://github.com/nathanfaucett/is_string.git"
  }
}
